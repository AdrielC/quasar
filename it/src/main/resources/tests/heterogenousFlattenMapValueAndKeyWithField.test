{
    "name": "field and flattened map field with value and key on heterogenous",
    "backends": {
        "couchbase":         "pending",
        "marklogic_json":    "pending",
        "marklogic_xml":     "pending",
        "mimir":             "ignoreFieldOrder"
    },
    "data": "heterogenousNested.data",
    "query": "select a, b{*}, b{*:} as k from heterogenousNested",
    "predicate": "exactly",
    "ignoreResultOrder": true,
    "expected": [
      { "a": 1, "b": 42, "k": "x" },
      { "a": 1, "b": 21, "k": "y" },
      { "a": 5, "b": "string", "k": "z" },
      { "a": 9, "b": [ 1, 2, 3 ], "k": "d" },
      { "a": 9, "b": { "n": 1 }, "k": "e" },
      { "a": 9, "b": null, "k": "f" },
      { "a": 9, "b": "foo", "k": "g" },
      { "a": 9, "b": {  }, "k": "h" },
      { "a": 9, "b": [], "k": "i" }
    ]
}
