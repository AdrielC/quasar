{
  "name": "postgres array manipulation tests",

  "backends": {
      "mongodb_read_only": "skip"
  },

  "data": "basic.data",

  "query": "select
  array_concat(array_concat(make_array(to_string(num)),make_array(name.last)),make_array(name.first))
   as res from basic",

  "predicate": "exactly",
  "ignoreResultOrder": true,

  "ignoredFields": [],

  "ignoreFieldOrder": true,

  "expected": [
    {"res": ["4",  "Smith",    "John"]},
    {"res": ["42", "Kowalska", "Janina"]}
  ]
}
